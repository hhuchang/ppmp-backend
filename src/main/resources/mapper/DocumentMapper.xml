<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.c2appstore.mapper.DocumentMapper">




    <select id="listByProjectIdAndParentId" resultType="cc.c2appstore.model.entity.Document">
        select * from tb_document where project_id=#{project_id} and parent_id=#{parent_id}
    </select>





</mapper>